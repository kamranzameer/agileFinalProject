<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<constant name="struts.enable.DynamicMethodInvocation" value="false" />

	<package name="wpm" extends="struts-default">

		<default-action-ref name="index" />

        <action name="workPackageList"  class="workPackageListAction">
            <result>/view/workspace.jsp</result>
        </action>

		<action name="helloWorld" class="helloWorldAction">
			<result name="input">/jsp/index.jsp</result>
			<result>/jsp/helloWorld.jsp</result>
		</action>

		<action name="workpackage" class="edu.harvard.agile.action.WorkPackageAction">
			<result name="success">view/workspace.jsp?p=wpl</result>
		</action>

	</package>

</struts>
